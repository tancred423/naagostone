{
  "SPECIAL_NOTICES": {
    "ROOT": {
      "selector": ".news__content.news__content__bottom-radius > ul.parts__space--add > li.news__list",
      "multiple": true
    },
    "TITLE": {
      "selector": "a.news__list--link > div.clearfix > p.news__list--title",
      "regex": "(?:<span.*>(?P<tag>.*)</span>)?(?P<title>.*)"
    },
    "DATE": {
      "selector": "a.news__list--link > div.clearfix > time.news__list--time > script",
      "regex": ".*ldst_strftime\\((?P<date>\\d*)"
    },
    "LINK": {
      "selector": "a.news__list--link",
      "attribute": "href"
    }
  },
  "REGULAR_NOTICES": {
    "ROOT": {
      "selector": ".news__content.news__content__bottom-radius > ul:not(.parts__space--add):not(.btn__pager) > li.news__list",
      "multiple": true
    },
    "TITLE": {
      "selector": "a.news__list--link > div.clearfix > p.news__list--title",
      "regex": "(?:<span.*>(?P<tag>.*)</span>)?(?P<title>.*)"
    },
    "DATE": {
      "selector": "a.news__list--link > div.clearfix > time.news__list--time > script",
      "regex": ".*ldst_strftime\\((?P<date>\\d*)"
    },
    "LINK": {
      "selector": "a.news__list--link",
      "attribute": "href"
    }
  }
}
